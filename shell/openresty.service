[Unit]
Description=nginx
After=network.target
  
[Service]
Type=forking
ExecStart=/usr/local/openresty/bin/openresty -p /www/PEMonitor
ExecReload=/usr/local/openresty/bin/openresty -p /www/PEMonitor -s reload
ExecStop=/usr/local/openresty/bin/openresty -p /www/PEMonitor -s stop
PrivateTmp=true
  
[Install]
WantedBy=multi-user.target
